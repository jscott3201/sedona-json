//
//

**
** WebParams models a list of HTTP name/value parameter.
**
class WebParams
{

////////////////////////////////////////////////////////////////
// Methods
////////////////////////////////////////////////////////////////

  void dump(OutStream out)
  {
    for (int i=0; i<size; ++i)
      out.print("   ").print(names[i]).print(": '").print(values[i]).print("'").nl()
  }

////////////////////////////////////////////////////////////////
// Fields
////////////////////////////////////////////////////////////////

  define int max = 32

  int size
  inline Str[max] names
  inline Str[max] values

}
