//
//

**
** Path models the parsed segments of the URI path
**
class Path
{

////////////////////////////////////////////////////////////////
// Methods
////////////////////////////////////////////////////////////////

  void dump(OutStream out)
  {
    foreach (Str name : names, size)
      out.print("'").print(name).print("' ");
  }

////////////////////////////////////////////////////////////////
// Fields
////////////////////////////////////////////////////////////////

  define int max = 32

  int size
  inline Str[max] names

}
